local function OMUTGN_fake_script() -- back.LocalScript 
	local script = Instance.new('LocalScript', back)

	local FrameObject = script.Parent -- Change Stats to whatever your frame is called
	local Open = false
	
	
	local UserInputService = game:GetService("UserInputService")
	
	UserInputService.InputBegan:Connect(function(Input, gameprocess)
		if not gameprocess then
			if Input.KeyCode == Enum.KeyCode.V then -- Change M to your prefered keybind
				if Open then
					Open = false
					script.Parent.Visible = true -- Change Stats to whatever your frame is called
